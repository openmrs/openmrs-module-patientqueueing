<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">
	
	<!-- Base Module Properties -->
	<id>${project.parent.artifactId}</id>
	<name>${project.parent.name}</name>
	<version>${project.parent.version}</version>
	<package>org.openmrs.module.patientqueueing</package>
	<author>METS</author>
	<description>
			${project.parent.description}
	</description>

	<activator>org.openmrs.module.patientqueueing.PatientQueueingActivator</activator>
	
	<!-- <updateURL>https://modules.openmrs.org/modules/download/@MODULE_ID@/update.rdf</updateURL> -->
	<!-- /Base Module Properties -->
	
	<require_version>${openmrsPlatformVersion}</require_version>

	<require_modules>
		<require_module version="${appframeworkVersion}">
			org.openmrs.module.appframework
		</require_module>
		<require_module version="${uiframeworkVersion}">
			org.openmrs.module.uiframework
		</require_module>
		<require_module version="${uicommonsModuleVersion}">
			org.openmrs.module.uicommons
		</require_module>
		<require_module version="${appuiVersion}">
			org.openmrs.module.appui
		</require_module>
		<require_module version="${coreappsVersion}">
			org.openmrs.module.coreapps
		</require_module>
	</require_modules>
	<!-- Extensions -->

	<aware_of_modules>
		<aware_of_module>org.openmrs.module.legacyui</aware_of_module>
	</aware_of_modules>
	
	
	<!-- Internationalization -->
	<!-- All message codes should start with @MODULE_ID@.* -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

	<privilege>
		<name>App: patientqueueing.accessClinicianDashboard</name>
		<description>Allows access to Clinician Dashboard.</description>
	</privilege>
	<privilege>
		<name>Task: patientqueueing.addPatientToQueue</name>
		<description>Ability to Add aPatient to a Queue.</description>
	</privilege>

	<privilege>
		<name>Task: patientqueueing.completePatientQueue</name>
		<description>Ability to Complete a Patient Queue.</description>
	</privilege>
</module>

